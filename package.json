{
  "name": "vera-project",
  "version": "1.0.0",
  "description": "**\"Not an AI pretending to be human, but a revolutionary intelligence built for your body.\"**",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:prod": "NODE_ENV=production node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "db:check": "node test-db.js",
    "db:init": "node init-database.js",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "db:backup": "node scripts/backup-database.js",
    "db:restore": "node scripts/restore-database.js",
    "cleanup:sessions": "node scripts/session-cleaner.js",
    "validate": "npm run lint && npm run test",
    "validate:env": "node -e \"require('./lib/env-validator').validate()\"",
    "deploy:check": "node scripts/deploy.js check",
    "deploy": "node scripts/deploy.js",
    "predeploy": "npm run validate:env",
    "docker:build": "docker build -t vera-project .",
    "docker:dev": "docker-compose up -d",
    "prepare": "husky",
    "health": "node -e \"require('http').get('http://localhost:8080/health', (r) => { let d = ''; r.on('data', (c) => d += c); r.on('end', () => console.log(d)); })\"",
    "health:railway": "node -e \"require('https').get('https://revolutionary-production.up.railway.app/health', (r) => { let d = ''; r.on('data', (c) => d += c); r.on('end', () => console.log(d)); })\"",
    "monitor": "node scripts/health-monitor.js",
    "backup": "node scripts/backup-database.js",
    "logs": "echo 'Check Railway dashboard: https://railway.app/'"
  },
  "keywords": [
    "ai",
    "nervous-system",
    "mental-health",
    "therapy",
    "chatbot",
    "claude",
    "somatic"
  ],
  "author": "VeraNeural",
  "license": "ISC",
  "type": "commonjs",
  "engines": {
    "node": ">=18 <23"
  },
